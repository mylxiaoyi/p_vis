<dataConfig>
	<datasetName>杭州房地产数据</datasetName>

	<attr>
		<fileName>hz_data/final_v0.9.3.csv</fileName>
		<fileType>csv</fileType>
		<key>smid</key>
		<name>楼盘</name>
		<attribute>
			<name>smid</name>
			<colIdx>1</colIdx>
			<dataType>id</dataType>
		</attribute>
		<attribute>
			<name>楼盘</name>
			<colIdx>2</colIdx>
			<dataType>string</dataType>
		</attribute>
		<attribute>
			<name>城区</name>
			<colIdx>3</colIdx>
			<dataType>string</dataType>
		</attribute>
		<attribute>
			<name>物业类型</name>
			<colIdx>4</colIdx>
			<dataType>string</dataType>
		</attribute>
		<attribute>
			<name>开发商</name>
			<colIdx>5</colIdx>
			<dataType>string</dataType>
		</attribute>
		<attribute>
			<name>总套数</name>
			<colIdx>6</colIdx>
			<dataType>int</dataType>
			<summaryType>sum</summaryType>
			<colorMap preset="OR_RD" />
		</attribute>
		<attribute>
			<name>总面积</name>
			<colIdx>7</colIdx>
			<dataType>double</dataType>
		</attribute>
		<attribute>
			<name>住宅套数</name>
			<colIdx>8</colIdx>
			<dataType>int</dataType>
		</attribute>
		<attribute>
			<name>住宅面积</name>
			<colIdx>9</colIdx>
			<dataType>double</dataType>
		</attribute>
		<attribute>
			<name>累积成</name>
			<colIdx>18</colIdx>
			<dataType>double</dataType>
			<summaryType>mean</summaryType>
			<colorMap preset="OR_RD" />
		</attribute>
		<attribute>
			<name>开盘次数</name>
			<colIdx>20</colIdx>
			<dataType>int</dataType>
			<summaryType>sum</summaryType>
			<colorMap preset="OR_RD" />
		</attribute>
		<hierSet>
			<hier>
				<name>城区</name>
				<hier>
					<name>楼盘</name>
				</hier>
			</hier>
		</hierSet>
	</attr>

	<geo>
		<feature>
			<name>楼盘</name>
			<geoType>point</geoType>
			<fileName>hz_data/smid_geo.csv</fileName>
			<fileType>csv</fileType>
			<col>
				<key>2</key>
				<x>4</x>
				<y>5</y>
			</col>
		</feature>
		<!-- <feature>
			<name>城区</name>
			<geoType>polygon</geoType>
			<fileName>map/44.json</fileName>
			<fileType>geojson</fileType>
		</feature> -->
		<feature>
			<name>规划单元</name>
			<geoType>polygon</geoType>
			<fileName>map/0.json</fileName>
			<fileType>geojson</fileType>
		</feature>
	</geo>
	<time>
		<fileName>sale_data_by_date.csv</fileName>
	</time>
</dataConfig>