<dataConfig>
  <datasetName>杭州房地产数据</datasetName>
  <attr>
    <fileName>hz_data/final_v0.9.3.csv</fileName>
    <fileType>csv</fileType>
    <key>smid</key>
    <name>楼盘</name>
    <attribute>
      <name>smid</name>
      <colIdx>1</colIdx>
      <dataType>id</dataType>
    </attribute>
    <attribute>
      <name>楼盘</name>
      <colIdx>2</colIdx>
      <dataType>string</dataType>
    </attribute>
    <attribute>
      <name>城区</name>
      <colIdx>3</colIdx>
      <dataType>string</dataType>
    </attribute>
    <attribute>
      <name>物业类型</name>
      <colIdx>4</colIdx>
      <dataType>string</dataType>
    </attribute>
    <attribute>
      <name>开发商</name>
      <colIdx>5</colIdx>
      <dataType>string</dataType>
    </attribute>
    <attribute>
      <name>总套数</name>
      <colIdx>6</colIdx>
      <dataType>int</dataType>
      <summaryType>sum</summaryType>
      <colorMap preset="OR_RD"/>
    </attribute>
    <attribute>
      <name>总面积</name>
      <colIdx>7</colIdx>
      <dataType>double</dataType>
    </attribute>
    <attribute>
      <name>住宅套数</name>
      <colIdx>8</colIdx>
      <dataType>int</dataType>
    </attribute>
    <attribute>
      <name>住宅面积</name>
      <colIdx>9</colIdx>
      <dataType>double</dataType>
    </attribute>
    <attribute>
      <name>毛坯均价</name>
      <colIdx>18</colIdx>
      <dataType>double</dataType>
      <summaryType>mean</summaryType>
      <colorMap preset="OR_RD"/>
    </attribute>
    <attribute>
      <name>开盘次数</name>
      <colIdx>20</colIdx>
      <dataType>int</dataType>
      <summaryType>sum</summaryType>
      <colorMap preset="OR_RD"/>
    </attribute>
    <hierSet>
      <hier>
        <name>城区</name>
        <hier>
          <name>楼盘</name>
        </hier>
      </hier>
    </hierSet>
  </attr>
  <geo>
    <feature>
      <name>楼盘</name>
      <geoType>point</geoType>
      <fileName>hz_data/smid_geo.csv</fileName>
      <fileType>csv</fileType>
      <col>
        <key>2</key>
        <x>4</x>
        <y>5</y>
      </col>
    </feature>
    <feature>
      <name>unit</name>
      <geoType>layers</geoType>
      <fileName>map/unit.json</fileName>
      <subDir>map/unit_layers</subDir>
      <fileType>geojson</fileType>
    </feature>
    <feature>
      <name>ghj_map</name>
      <geoType>layers</geoType>
      <fileName>map/ghj_map.json</fileName>
      <subDir>map/ghj_map_layers</subDir>
      <fileType>geojson</fileType>
    </feature>
  </geo>
  <time>
    <series>
      <fileName>sale_data_by_date.csv</fileName>
      <dateCol>0</dateCol>
      <groupCol>1</groupCol>
      <valueCol>4</valueCol>
    </series>
    <series>
      <fileName>sale_data_by_date.csv</fileName>
      <dateCol>0</dateCol>
      <groupCol>1</groupCol>
      <valueCol>4</valueCol>
    </series>
  </time>
</dataConfig>